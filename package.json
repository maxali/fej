{
  "name": "fej",
  "version": "1.1.0",
  "description": "Fetch API with middlewares - Modern, lightweight, zero dependencies. Named middleware, retry logic, AbortController support, and more!",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"*.json\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.json\" \"*.md\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check:size": "node scripts/check-bundle-size.js",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "prepublishOnly": "npm run lint && npm run type-check && npm run test && npm run build && npm run check:size",
    "clean": "rimraf dist docs"
  },
  "keywords": [
    "Fetch",
    "Middleware"
  ],
  "author": "maxali",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "expect-type": "^1.2.2",
    "prettier": "^3.0.0",
    "typescript-eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxali/fej.git"
  },
  "bugs": {
    "url": "https://github.com/maxali/fej/issues"
  },
  "homepage": "https://github.com/maxali/fej#readme"
}
